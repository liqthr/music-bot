{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/callback",
      "dest": "/public/callback.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/auth",
      "dest": "/server.js"
    },
    {
      "src": "/spotify-search",
      "dest": "/server.js"
    },
    {
      "src": "/soundcloud-search",
      "dest": "/server.js"
    },
    {
      "src": "/soundcloud-search-alt",
      "dest": "/server.js"
    },
    {
      "src": "/health",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}