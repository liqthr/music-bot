{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "headers": [
    {
      "source": "/public/(.*)\\.(css|js|png|jpg|jpeg|svg|webp|gif|woff2)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/(.*)\\.(css|js|png|jpg|jpeg|svg|webp|gif|woff2)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "routes": [
    {
      "src": "/callback",
      "dest": "/public/callback.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/auth",
      "dest": "/server.js"
    },
    {
      "src": "/search",
      "dest": "/server.js"
    },
    {
      "src": "/youtube-search",
      "dest": "/server.js"
    },
    {
      "src": "/youtube-video/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/spotify-search",
      "dest": "/server.js"
    },
    {
      "src": "/soundcloud-search",
      "dest": "/server.js"
    },
    {
      "src": "/soundcloud-search-alt",
      "dest": "/server.js"
    },
    {
      "src": "/health",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}